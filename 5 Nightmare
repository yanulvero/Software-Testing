2.BRING IT ON
cd "d:/Лабораторные/Тестирование/1/Software-Testing"
echo "*.db
*.log
/target
/bin" > .gitignore
git add .
git commit -m "commit ignore file"
git push
git checkout -b feature
copy con "d:/Лабораторные/Тестирование/1/Software-Testing/album.txt"
git add .
git commit -m "create album.txt"
copy con "d:/Лабораторные/Тестирование/1/Software-Testing/proof.txt"
git add .
git commit -m "create proof.txt"
git checkout main
git push origin feature
git merge feature



git checkout feature
cd "d:/Лабораторные/Тестирование/1/Software-Testing"
echo "The ship glides gently on the waves
As day turns into night" > arrows.txt
git add .
git commit -m "arrows.txt feature"
git checkout main
cd "d:/Лабораторные/Тестирование/1/Software-Testing"
echo "One thousand burning arrows
Fill the starlit sky" > arrows.txt
git add .
git commit -m "arrows.txt main"
git merge feature
echo "The ship glides gently on the waves
As day turns into night
One thousand burning arrows
Fill the starlit sky" > arrows.txt
git status
git add .
git commit -m "branch main and feature merge. One arrows.txt version"
git push
git checkout feature


